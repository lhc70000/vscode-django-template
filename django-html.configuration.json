{
    "comments": {
        "blockComment": [
            "{% comment %}",
            "{% endcomment %}"
        ]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
       { "open": "{{ ", "close": " }}" },
       { "open": "{% ", "close": " %}" },
